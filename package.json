{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run angular-starter:serve-ssr",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod && ng run angular-starter:server:production",
    "serve:stats": "webpack-bundle-analyzer dist/angular-starter/browser/stats-es2015.json",
    "serve:prod": "http-server -p 8000 -c-1 dist/angular-starter/browser --proxy http://localhost:8000?",
    "serve:ssr": "node dist/angular-starter/server/main.js",
    "test": "ng test",
    "test:without-watch": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run angular-starter:prerender",
    "analyze": "npm run build:stats && npm run serve:stats",
    "format": "prettier --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:watch": "onchange \"**/*.{ts,js,css,scss,html,json,md}\" -- prettier --write {{changed}}",
    "format:check": "prettier --check \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:staged": "pretty-quick --staged --pattern \"**/*.{ts,js,css,scss,html,json,md}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/platform-server": "~11.2.11",
    "@angular/router": "~11.2.11",
    "@angular/service-worker": "~11.2.11",
    "@avivharuzi/ngx-seo": "^10.0.0",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@nguniversal/express-engine": "^11.2.1",
    "express": "^4.15.2",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
