{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run angular-starter:serve-ssr",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:ssr": "ng build && ng run angular-starter:server",
    "build:stats": "ng build --stats-json",
    "prerender": "ng run angular-starter:prerender",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:no-watch": "ng test --no-watch",
    "test:code-coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "serve": "http-server -p 8000 -c-1 dist/angular-starter/browser --proxy http://localhost:8000?",
    "serve:ssr": "node dist/angular-starter/server/main.js",
    "serve:stats": "webpack-bundle-analyzer dist/angular-starter/browser/stats-es2015.json",
    "analyze": "npm run build:stats && npm run serve:stats",
    "cz": "git-cz",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/platform-server": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@angular/service-worker": "~13.0.0",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.15.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-cz": "^4.8.0",
    "http-server": "^14.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "typescript": "~4.4.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
