{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run angular-starter:serve-ssr",
    "build": "NODE_ENV=production ng build",
    "build:dev": "ng build --configuration development",
    "build:ssr": "NODE_ENV=production ng build && ng run angular-starter:server",
    "build:stats": "NODE_ENV=production ng build --stats-json",
    "prerender": "ng run angular-starter:prerender",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:without-watch": "ng test --watch false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:watch": "onchange \"**/*.{ts,js,css,scss,html,json,md}\" -- prettier --write {{changed}}",
    "format:check": "prettier --check \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:staged": "pretty-quick --staged --pattern \"**/*.{ts,js,css,scss,html,json,md}\"",
    "serve": "http-server -p 8000 -c-1 dist/angular-starter/browser --proxy http://localhost:8000?",
    "serve:ssr": "node dist/angular-starter/server/main.js",
    "serve:stats": "webpack-bundle-analyzer dist/angular-starter/browser/stats-es2015.json",
    "analyze": "npm run build:stats && npm run serve:stats"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/platform-server": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@angular/service-worker": "~12.0.0",
    "@avivharuzi/ngx-seo": "^12.0.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@nguniversal/express-engine": "^12.0.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@nguniversal/builders": "^12.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "onchange": "^7.1.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "^2.1.2",
    "typescript": "~4.2.3",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
